<%= form_for @listing do |f| %>
  <%= f.label :property_name %>:
  <%= f.text_field :property_name %><br />

  <%= f.label :country %>:
  <%= f.text_field :country %><br />

  <%= f.label :city_town %>:
  <%= f.text_field :city_town %><br />

  <%= f.label :home_type %>:
  <%= f.text_field :home_type %><br />

  <%= f.label :guest %>:
  <%= f.text_field :guest %><br />

  <%= f.label :description %>:
  <%= f.text_field :description %><br />

  <%= f.label :price %>:
  <%= f.text_field :price %><br />

  <%= f.file_field :photos, multiple: true %><br />

  <%= f.submit %>
<% end %>